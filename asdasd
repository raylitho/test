-- Main Loop
RunService.Heartbeat:Connect(function()
    updateCharacter()
    autoCollectLoop()
    candyAutofarmLoop()
    spawnXenoBundle()
    spawnAlienbeam()
end)

-- Initialize ESP
updateESP()

